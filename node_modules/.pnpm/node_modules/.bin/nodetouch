#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/boot/Public/www/htmx-notes/node_modules/.pnpm/touch@3.1.0/node_modules/touch/bin/nodetouch.js/node_modules:/home/boot/Public/www/htmx-notes/node_modules/.pnpm/touch@3.1.0/node_modules/touch/bin/node_modules:/home/boot/Public/www/htmx-notes/node_modules/.pnpm/touch@3.1.0/node_modules/touch/node_modules:/home/boot/Public/www/htmx-notes/node_modules/.pnpm/touch@3.1.0/node_modules:/home/boot/Public/www/htmx-notes/node_modules/.pnpm/node_modules:/home/boot/Public/www/htmx-notes/node_modules:/home/boot/Public/www/node_modules:/home/boot/Public/node_modules:/home/boot/node_modules:/home/node_modules:/node_modules:/home/boot/Public/www/htmx-notes/node_modules/.pnpm/node_modules/touch/bin/nodetouch.js/node_modules:/home/boot/Public/www/htmx-notes/node_modules/.pnpm/node_modules/touch/bin/node_modules:/home/boot/Public/www/htmx-notes/node_modules/.pnpm/node_modules/touch/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else 
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
